{
  "name": "my-project",
  "description": "h5",
  "keywords": [
    "vue-h5",
    "vue-cli",
    "@vue/CLI3"
  ],
  "version": "2.0.13",
  "scripts": {
    "report": "node build/index.js --preview --report",
    "preview": "node build/server.js",
    "dev": "vue-cli-service serve",
    "build:test": "vue-cli-service build --mode test",
    "build:dev": "vue-cli-service build --mode development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "prettier": "prettier --write \"src/**/*.{vue,js,less,css}\"",
    "new": "plop",
    "api": "plop api",
    "release": "sh build/pre-release.sh",
    "changelog": "sh build/git-log.sh -m changelog -f"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/register": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "3.12.0",
    "@vue/cli-plugin-eslint": "3.12.0",
    "@vue/cli-service": "3.12.0",
    "@vue/eslint-config-standard": "4.0.0",
    "axios": "0.19.0",
    "babel-eslint": "10.0.3",
    "babel-plugin-component": "1.1.1",
    "babel-polyfill": "6.26.0",
    "clipboard": "^2.0.4",
    "core-js": "2.6.10",
    "es6-promise": "^4.2.8",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fastclick": "1.0.6",
    "js-base64": "^2.5.2",
    "less": "3.10.3",
    "less-loader": "4.1.0",
    "lib-flexible": "0.3.2",
    "postcss-pxtorem": "4.0.1",
    "sa-sdk-javascript": "1.14.13",
    "sharegoods-utils": "0.0.4",
    "style-resources-loader": "1.2.1",
    "vant": "2.2.9",
    "vue": "2.6.10",
    "vue-router": "3.1.3",
    "vue-seamless-scroll": "^1.1.17",
    "vue-template-compiler": "2.6.10",
    "vue-wechat-title": "^2.0.5",
    "vuex": "3.1.1",
    "vuex-persistedstate": "2.6.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "weixin-jsapi": "^1.1.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.2",
    "chalk": "^2.4.2",
    "chokidar": "^3.2.2",
    "connect": "^3.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mockjs": "^1.0.1-beta3",
    "plop": "^2.5.2",
    "prettier": "^1.18.2",
    "require-all": "^3.0.0",
    "runjs": "^4.4.2",
    "serve-static": "^1.14.1",
    "terser-webpack-plugin": "^2.2.1",
    "vue-lazyload": "^1.3.3"
  }
}
